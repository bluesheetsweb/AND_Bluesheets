<?xml version="1.0" encoding="utf-8"?>
<src.wrapperutil.uicomponent.LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center_vertical"
    app:stroke_width="@dimen/_1dp"
    app:corner_radius="@dimen/_24dp"
    app:stroke_color="SECONDARY"
    app:background_state="WHITE"
    android:orientation="horizontal">

    <src.wrapperutil.uicomponent.TextView
        android:id="@+id/tv_tag_item_contain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/_16dp"
        android:paddingRight="@dimen/_16dp"
        android:paddingTop="@dimen/_8dp"
        android:paddingBottom="@dimen/_8dp"
        android:gravity="center"
        app:text_mode="SUB_HEADING_REGULAR_SECONDARY"/>
</src.wrapperutil.uicomponent.LinearLayout>